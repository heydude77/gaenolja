<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="DTBOARD">

<!--  리스트 뽑기 -->
<select id="getAlldt" resultType="java.util.HashMap">
   select *from DTBOARD order by NUM desc
 </select>
 
 <!-- 조회 -->
<select id="readdt" resultType="java.util.HashMap">
   select*from DTBOARD where NUM=#{NUM}
   </select>
   
 <!-- 글쓰고 저장 -->
 <insert id="adddtboard" parameterType="java.util.HashMap">
   insert into DTBOARD values(DTBOARD_SEQ.nextval,#{SUB},#{CON},#{DTBOARDID},sysdate,0)
 </insert>
   
   
  	 </mapper>